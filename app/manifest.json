{
  "name": "__MSG_appName__",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "homepage_url": "https://github.com/brenoc/github-control",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js",
      "scripts/libs/base64.js",
      "scripts/libs/underscore-min.js",
      "scripts/libs/github.js"
    ]
  },
  "options_page": "options/index.html",
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "GitHub Control",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "storage",
    "identity",
    "https://github.com/"
  ]
}